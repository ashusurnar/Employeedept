 @model IEnumerable< MarotiDemo.Models.AddViewModel.EmpDataSummary>


 <div class="container">
     <div class="row">
         <div class="col-8 offset-2">
             <h2 class="text-center text-black">Employee</h2>
             <div>
                 <a class="btn btn-outline-success" asp-action="Create" asp-controller="Employee">Create</a>
             </div>
             <div>
                 <table class="table table-bordered">
                     <tr>
                         <th>Empid</th>
                        <th>EmpName</th>
                        <th>City</th>
                        <th>Salary</th>
                        <th>Deptid</th>
                        <th>DeptName</th>
                        <th>Action</th>
                     </tr>

                     @foreach(var emp in Model)
                    {
                        <tr>
                            <td>@emp.empid</td>
                            <td>@emp.empname</td>
                            <td>@emp.city</td>
                            <td>@emp.salary</td>
                            <td>@emp.deptid</td>
                            <td>@emp.deptname</td>
                            <td>
                                <a class="btn btn-outline-warning" asp-action="Edit" asp-controller="Employee" asp-route-id="@emp.empid">Edit</a>
                                <a class="btn btn-outline-danger" asp-action="Delete" asp-controller="Employee" asp-route-id="@emp.empid">Delete</a>
                            </td>

                        </tr>
                        
                    }

                 </table>
             </div>
         </div>

     </div>
 </div>